<project>
 <modelVersion>4.0.0</modelVersion>
   <groupId>com.newarchetype</groupId>
   <artifactId>test-boot</artifactId>
   <packaging>jar</packaging>
    <name>Crap JBI Component Bootstrap</name>
   <version>1.0-SNAPSHOT</version>
  <description>Crap JBI Component Bootstrap</description>
  <dependencies>
      <dependency>
        <groupId>javax.jbi</groupId>
        <artifactId>jbi</artifactId>
        <version>1.0</version>
      </dependency>
    </dependencies>
    <build>
      <plugins>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-compiler-plugin</artifactId>
            <configuration>
              <source>1.5</source>
              <target>1.5</target>
            </configuration>
        </plugin>
      </plugins>
  </build>  
</project>
