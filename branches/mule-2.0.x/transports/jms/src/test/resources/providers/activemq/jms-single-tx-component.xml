<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0"
      xmlns:jms="http://www.mulesource.org/schema/mule/jms/2.0"
      xmlns:test="http://www.mulesource.org/schema/mule/test/2.0"
      xsi:schemaLocation="
       http://www.mulesource.org/schema/mule/test/2.0 http://www.mulesource.org/schema/mule/test/2.0/mule-test.xsd
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
       http://www.mulesource.org/schema/mule/jms/2.0 http://www.mulesource.org/schema/mule/jms/2.0/mule-jms.xsd">

    <jms:activemq-connector name="jmsConnector"/>

    <model name="jmsTest">
        <service name="Part1">
            <inbound>
                <jms:inbound-endpoint queue="in">
                    <jms:transaction action="BEGIN_OR_JOIN"/>
                </jms:inbound-endpoint>
            </inbound>
            <test:component/>
            <outbound>
                <outbound-pass-through-router>
                    <jms:outbound-endpoint queue="middle"/>
                </outbound-pass-through-router>
            </outbound>
        </service>

        <service name="Part2">
            <inbound>
                <jms:inbound-endpoint queue="middle">
                    <jms:transaction action="BEGIN_OR_JOIN"/>
                </jms:inbound-endpoint>
            </inbound>
            <test:component/>
            <outbound>
                <outbound-pass-through-router>
                    <jms:outbound-endpoint queue="q1">
                        <jms:transaction action="BEGIN_OR_JOIN"/>
                    </jms:outbound-endpoint>
                </outbound-pass-through-router>
            </outbound>
        </service>

        <service name="Part3">
            <inbound>
                <jms:inbound-endpoint queue="q1">
                    <jms:transaction action="BEGIN_OR_JOIN"/>
                </jms:inbound-endpoint>
            </inbound>
            <test:component/>
            <outbound>
                <outbound-pass-through-router>
                    <jms:outbound-endpoint queue="q2">
                        <jms:transaction action="BEGIN_OR_JOIN"/>
                    </jms:outbound-endpoint>
                </outbound-pass-through-router>
            </outbound>
        </service>

        <service name="Part4">
            <inbound>
                <jms:inbound-endpoint queue="q2">
                    <jms:transaction action="BEGIN_OR_JOIN"/>
                </jms:inbound-endpoint>
            </inbound>
            <test:component>
                <test:return-data>OUTPUT MESSAGE</test:return-data>
            </test:component>
            <outbound>
                <outbound-pass-through-router>
                    <jms:outbound-endpoint queue="out">
                        <jms:transaction action="BEGIN_OR_JOIN"/>
                    </jms:outbound-endpoint>
                </outbound-pass-through-router>
            </outbound>
        </service>
    </model>
</mule>