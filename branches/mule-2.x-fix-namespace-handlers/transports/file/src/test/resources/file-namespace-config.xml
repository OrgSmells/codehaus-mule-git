<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:file="http://www.mulesource.org/schema/mule/file/2.0"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.mulesource.org/schema/mule/core/2.0 META-INF/mule.xsd
       http://www.mulesource.org/schema/mule/file/2.0 META-INF/mule-file.xsd">

    <file:connector name="fileConnector"
            autoDelete="false"
            fileAge="1234"
            moveToDirectory="abc"
            moveToPattern="bcd"
            outputAppend="true"
            outputPattern="cde"
            pollingFrequency="2345"
            serialiseObjects="true">
        <!-- not tested as they force directory creation -->
      <!-- readFromDirectory="efg"
           writeToDirectory="fgh" -->
        <file:filename-parser class="org.mule.providers.file.DummyFilenameParser"/>
    </file:connector>

    <file:connector name="secondConnector">
        <file:filename-parser class="org.mule.providers.file.SecondDummyFilenameParser"/>
    </file:connector>

</beans>