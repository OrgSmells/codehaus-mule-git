    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/http/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" xmlns:tcp="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:spring="http://www.springframework.org/schema/beans" name="http:connector" type="http:httpConnectorType" substitutionGroup="mule:abstract-connector"><xsd:annotation>
