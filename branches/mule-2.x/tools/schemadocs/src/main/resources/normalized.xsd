    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" name="tcp:eof-protocol" type="tcp:byteOrMessageProtocolType" substitutionGroup="tcp:abstract-protocol"/>
    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" name="tcp:direct-protocol" type="tcp:byteOrMessageProtocolType" substitutionGroup="tcp:abstract-protocol"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" name="tcp:byteOrMessageProtocolType"><xsd:complexContent><xsd:extension base="tcp:abstractProtocolType"><xsd:attribute name="payloadOnly" type="mule:substitutableBoolean"/></xsd:extension></xsd:complexContent></xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" name="tcp:safe-protocol" type="tcp:lengthProtocolType" substitutionGroup="tcp:abstract-protocol"/>
    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" name="tcp:length-protocol" type="tcp:lengthProtocolType" substitutionGroup="tcp:abstract-protocol"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" name="tcp:lengthProtocolType"><xsd:complexContent><xsd:extension base="tcp:byteOrMessageProtocolType"><xsd:attribute name="maxMessageLength" type="mule:substitutableInt"><xsd:annotation>
                        <xsd:documentation>
                            An optional maximum length for the number of bytes in a single message.
                            Messages larger than this will trigger an error in the receiver, but
                            give an assurance that no out-of-memory error will occur.
                        </xsd:documentation>
                    </xsd:annotation></xsd:attribute></xsd:extension></xsd:complexContent></xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mulesource.org/schema/mule/tcp/2.0" xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0" name="tcp:custom-protocol" type="tcp:customProtocolType" substitutionGroup="tcp:abstract-protocol"/>
