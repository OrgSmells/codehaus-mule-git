<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:spring="http://www.springframework.org/schema/beans"
       xmlns:jms="http://www.mulesource.org/schema/mule/jms/2.0"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
       http://www.mulesource.org/schema/mule/jms/2.0 http://www.mulesource.org/schema/mule/jms/2.0/mule-jms.xsd">

    <model name="testModel" class="org.mule.tck.testmodels.mule.TestSedaModel">

        <service name="queueComponent">
            <component class="org.mule.tck.functional.FunctionalTestComponent" scope="singleton"/>
            <inbound-router>
                <inbound-endpoint address="jms://in"/>
            </inbound-router>
            <outbound-router>
                <outbound-pass-through-router>
                    <outbound-endpoint address="jms://out"/>
                </outbound-pass-through-router>
            </outbound-router>
        </service>

        <service name="topicComponent">
            <component class="org.mule.tck.functional.FunctionalTestComponent" scope="singleton"/>
            <inbound-router>
                <inbound-endpoint address="jms://topic:in"/>
            </inbound-router>
            <outbound-router>
                <outbound-pass-through-router>
                    <outbound-endpoint address="jms://topic:out"/>
                </outbound-pass-through-router>
            </outbound-router>
        </service>
    </model>

</mule>