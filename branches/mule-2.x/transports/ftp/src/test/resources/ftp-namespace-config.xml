<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:mule="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:ftp="http://www.mulesource.org/schema/mule/ftp/2.0"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
       http://www.mulesource.org/schema/mule/ftp/2.0 META-INF/mule-ftp.xsd">

    <ftp:connector name="ftpConnector"
            binary="false"
            outputPattern="abc"
            passive="false"
            pollingFrequency="1234"
            validateConnections="false">
        <ftp:filename-parser class="org.mule.providers.file.DummyFilenameParser"/>
    </ftp:connector>

</beans>