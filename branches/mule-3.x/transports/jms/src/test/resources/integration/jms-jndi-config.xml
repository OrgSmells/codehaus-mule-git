<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:util="http://www.springframework.org/schema/util"
      xmlns:jms="http://www.mulesource.org/schema/mule/jms"
      xmlns:vm="http://www.mulesource.org/schema/mule/vm"
      xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
           http://www.mulesource.org/schema/mule/core http://www.mulesource.org/schema/mule/core/3.0/mule.xsd
           http://www.mulesource.org/schema/mule/vm http://www.mulesource.org/schema/mule/vm/3.0/mule-vm.xsd
           http://www.mulesource.org/schema/mule/jms http://www.mulesource.org/schema/mule/jms/3.0/mule-jms.xsd">

    <vm:connector name="vmConnector"/>

    <model>
        <service name="QueueFromJndi">
            <inbound>
                <!-- this queue is looked up from JNDI -->
                <inbound-endpoint ref="ep_jndi-queue" />
            </inbound>
            <outbound>
                <pass-through-router>
                    <vm:outbound-endpoint path="out"/>
                </pass-through-router>
            </outbound>
        </service>

        <service name="TopicFromJndi">
            <inbound>
                <!-- this topic is looked up from JNDI -->
                <inbound-endpoint ref="ep_jndi-topic" />
            </inbound>
            <outbound>
                <pass-through-router>
                    <vm:outbound-endpoint path="out"/>
                </pass-through-router>
            </outbound>
        </service>

        <service name="NonJndiQueue">
            <inbound>
                <inbound-endpoint ref="ep_non-jndi-queue" />
            </inbound>
            <outbound>
                <pass-through-router>
                    <vm:outbound-endpoint path="out"/>
                </pass-through-router>
            </outbound>
        </service>
    </model>
</mule>
