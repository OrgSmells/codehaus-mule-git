<!--
Assembly descriptor for the YourKit Profiler distribution.
-->
<assembly>
    <id>yjp</id>
    <formats>
        <format>zip</format>
        <format>tar.gz</format>
    </formats>

    <includeBaseDirectory>false</includeBaseDirectory>

    <!--
    ============================
        Libraries
    ============================
    -->

    <dependencySets>

        <!-- Non-Mule libraries -->
        <dependencySet>
            <outputDirectory>lib/user</outputDirectory>
            <scope>runtime</scope>
            <excludes>
                <exclude>backport-util-concurrent:backport-util-concurrent</exclude>
            </excludes>
        </dependencySet>
    </dependencySets>

</assembly>
