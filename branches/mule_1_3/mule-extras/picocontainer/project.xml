<?xml version="1.0" encoding="UTF-8"?>

<project>
    <extend>${basedir}/../project.xml</extend>
    <groupId>mule</groupId>
    <id>mule-extras-picocontainer</id>
    <artifactId>mule-extras-picocontainer</artifactId>
    <name>Pico Extensions</name>
    <shortDescription>pico</shortDescription>
    <description>Provides extensions to use PicoContainer with Mule, such as using the container as a factory for Mule managed objects. for more information see <a href="http://wiki.muleumo.org/display/MULE/Mule+Extras">Mule Extras</a>.</description>
    <dependencies>
        <dependency>
            <groupId>picocontainer</groupId>
            <artifactId>picocontainer</artifactId>
            <version>${picocontainer_version}</version>
            <url>http://www.picocontainer.org</url>
        </dependency>
        <dependency>
            <artifactId>nanocontainer</artifactId>
            <groupId>nanocontainer</groupId>
            <version>${nanocontainer_version}</version>
            <url>http://www.picocontainer.org</url>
        </dependency>
        <dependency>
            <groupId>nanocontainer</groupId>
            <artifactId>nanocontainer-nanowar</artifactId>
            <version>${nanocontainer_nanowar_version}</version>
            <url>http://www.picocontainer.org</url>
        </dependency>
    </dependencies>
</project>