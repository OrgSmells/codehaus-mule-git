<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:spring="http://www.springframework.org/schema/beans"
       xmlns:jms="http://www.mulesource.org/schema/mule/jms/2.0"
       xmlns:test="http://www.mulesource.org/schema/mule/jms-test/2.0"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
       http://www.mulesource.org/schema/mule/jms/2.0 http://www.mulesource.org/schema/mule/jms/2.0/mule-jms.xsd
       http://www.mulesource.org/schema/mule/jms-test/2.0 http://www.mulesource.org/schema/mule/jms/2.0/mule-jms-test.xsd">

    <jms:activemq-connector name="activeMqConnectorDefaults"/>
    <jms:activemq-connector name="activeMqConnectorBroker" brokerURL="tcp://localhost:1234"/>
    
    <jms:weblogic-connector name="weblogicConnector">
        <jms:connection-factory class="org.mule.providers.jms.test.TestConnectionFactory"/>
    </jms:weblogic-connector>

    <jms:websphere-connector name="websphereConnector">
        <jms:connection-factory class="org.mule.providers.jms.test.TestConnectionFactory"/>
    </jms:websphere-connector>

</mule>
