<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:spring="http://www.springframework.org/schema/beans"
       xmlns:annotations="http://www.mulesource.org/schema/mule/annotations/2.0"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.mulesource.org/schema/mule/annotations/2.0 http://www.mulesource.org/schema/mule/annotations/2.0/mule-annotations.xsd
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd">

    <model name="main">
        <annotations:service name="hello" class="org.mule.config.annotations.example.AnnotatedHelloComponent"/>
        <annotations:service name="greeter" class="org.mule.config.annotations.example.GreetingComponent"/>
    </model>

</mule>