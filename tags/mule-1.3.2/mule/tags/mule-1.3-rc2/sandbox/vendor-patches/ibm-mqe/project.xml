<?xml version="1.0" encoding="UTF-8"?>
<project>
    <extend>${basedir}/../../project.xml</extend>
    <groupId>mule</groupId>
  	<artifactId>mule-vendor-ibmmqe</artifactId>
  	<name>IBM MQe support</name>
  	<dependencies>
        <dependency>
            <artifactId>mule-jms-provider</artifactId>
            <groupId>mule</groupId>
            <version>${pom.currentVersion}</version>
            <url>http://www.muleumo.org</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
            	<eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>
        <dependency>
            <groupId>geronimo-spec</groupId>
            <artifactId>geronimo-spec-jms</artifactId>
            <version>${geronimo_spec_jms_version}</version>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <mule.mini>true</mule.mini>
                <rar.bundle>false</rar.bundle>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
	    <dependency>
	      <groupId>MQeBase</groupId>
	      <artifactId>MQeBase</artifactId>
	      <jar>${basedir}/main/lib/MQe/MQeBase.jar</jar>
	      <type>jar</type>
	    </dependency>
	    <dependency>
	      <groupId>MQeJMS</groupId>
	      <artifactId>MQeJMS</artifactId>
	      <jar>${basedir}/main/lib/MQe/MQeJMS.jar</jar>
	      <type>jar</type>
	    </dependency>
  	</dependencies>
</project>

