<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.codehaus.mule</groupId>
        <artifactId>mule-providers</artifactId>
        <version>2.0-SNAPSHOT</version>
    </parent>
    <artifactId>mule-oracle-jms-provider</artifactId>
    <packaging>jar</packaging>
    <name>Oracle Advanced Queuing Provider</name>
    <description>Adds Oracle-specific functionality to the standard Mule JMS provider.</description>
    <dependencies>
        <dependency>
            <groupId>org.codehaus.mule</groupId>
            <artifactId>mule-jms-provider</artifactId>
            <version>${version}</version>
        </dependency>
        <dependency>
            <groupId>oracle</groupId>
            <artifactId>ojdbc14</artifactId>
            <version>9i</version>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>oracle</groupId>
            <artifactId>jmscommon</artifactId>
            <version>9i</version>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>oracle</groupId>
            <artifactId>aqapi13</artifactId>
            <version>9i</version>
            <optional>true</optional>
        </dependency>

        <!-- For XMLType support only -->
        <dependency>
            <groupId>oracle</groupId>
            <artifactId>xdb</artifactId>
            <version>9i</version>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>oracle</groupId>
            <artifactId>xmlparserv2</artifactId>
            <version>9i</version>
            <optional>true</optional>
        </dependency>
    </dependencies>
</project>
