<?xml version="1.0"?>
<project>
    <extend>${basedir}/../project.xml</extend>
    <pomVersion>3</pomVersion>
    <artifactId>mule-jdbc-provider</artifactId>
    <name>Mule Jdbc Provider</name>
    <groupId>mule</groupId>
    <description>A Mule provider for Jdbc Connectivity. for more information see the
        <a href="http://wiki.muleumo.org/display/MULE/Providers+Guide">Providers Guide</a>.
    </description>
    <shortDescription>email</shortDescription>
    <url>http://www.muleumo.org/doc/providers/jdbc/</url>
    <siteDirectory>/home/http/symphonysoft.com/html/mule/reference/providers/jdbc/</siteDirectory>
    <dependencies>
        <dependency>
            <groupId>commons-dbutils</groupId>
            <artifactId>commons-dbutils</artifactId>
            <version>${commons_dbutils_version}</version>
            <url>http://jakarta.apache.org/commons/dbutils/index.html</url>
        </dependency>
        <dependency>
            <groupId>xapool</groupId>
            <artifactId>xapool</artifactId>
            <version>${xapool_version}</version>
            <type>jar</type>
            <url>http://xapool.experlog.com/</url>
            <properties/>
        </dependency>
        <dependency>
        	<groupId>hsqldb</groupId>
        	<artifactId>hsqldb</artifactId>
        	<version>${hsqldb_version}</version>
        	<url>http://hsqldb.sourceforge.net/</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
    </dependencies>
</project>
