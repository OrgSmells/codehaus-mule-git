<?xml version="1.0"?>

<project>
    <extend>${basedir}/../project2.xml</extend>
    <pomVersion>3</pomVersion>
    <artifactId>mule-xfire-provider</artifactId>
    <name>Mule Xfire Provider</name>
    <description>A Mule provider for Soap Connectivity using XFire, a STaX-based soap stack.
        <a href="http://mule.codehaus.org/Transports+Guide">Transports Guide</a>.
    </description>
    <shortDescription>soap</shortDescription>
    <url>http://www.muleumo.org/doc/providers/xfire/</url>
    <dependencies>
        <dependency>
            <id>commons-httpclient</id>
            <version>${commons_httpclient_version}</version>
            <url>http://jakarta.apache.org/commons/httpclient/index.html</url>
            <properties>
                <war.bundle.jar>true</war.bundle.jar>
            </properties>
        </dependency>

        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-soap-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <url>http://www.muleumo.org</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
            	<eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>

        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-http-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <url>http://www.muleumo.org</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
            	<eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>

        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-extras-client</artifactId>
            <version>${pom.currentVersion}</version>
            <url>http://www.muleumo.org</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
            	<eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>

        <!-- XFire Deps -->
        <dependency>
            <groupId>stax</groupId>
            <artifactId>stax</artifactId>
            <version>${stax_version}</version>
            <url>http://jcp.org/en/jsr/detail?id=173</url>
        </dependency>

        <dependency>
            <groupId>stax</groupId>
            <artifactId>stax-api</artifactId>
            <version>${stax_api_version}</version>
            <url>http://jcp.org/en/jsr/detail?id=173</url>
        </dependency>

        <dependency>
            <groupId>wsdl4j</groupId>
            <artifactId>wsdl4j</artifactId>
            <version>${wsdl4j_version}</version>
            <url>http://www-124.ibm.com/developerworks/projects/wsdl4j/</url>
        </dependency>

        <!--dependency>
            <groupId>xfire</groupId>
            <artifactId>xfire-core</artifactId>
            <version>${xfire_core_version}</version>
            <url>http://xfire.codehaus.org</url>
        </dependency>

        <dependency>
            <groupId>xfire</groupId>
            <artifactId>xfire-aegis</artifactId>
            <version>${xfire_aegis_version}</version>
            <url>http://xfire.codehaus.org</url>
        </dependency-->

        <dependency>
            <groupId>xfire</groupId>
            <artifactId>xfire-all</artifactId>
            <version>${xfire_all_version}</version>
            <url>http://xfire.codehaus.org</url>
        </dependency>

        <dependency>
            <groupId>woodstox</groupId>
            <artifactId>wstx-asl</artifactId>
            <version>${wstx_asl_version}</version>
             <url>http://woodstox.codehaus.org</url>
        </dependency>

        <dependency>
            <groupId>jdom</groupId>
            <artifactId>jdom</artifactId>
            <version>${jdom_version}</version>
        </dependency>

        <dependency>
            <groupId>jaxen</groupId>
            <artifactId>jaxen</artifactId>
            <version>${jaxen_version}</version>
        </dependency>

        <dependency>
            <groupId>geronimo-spec</groupId>
            <artifactId>geronimo-spec-javamail</artifactId>
            <version>${geronimo_spec_javamail_version}</version>
            <url>http://java.sun.com/products/javamail/</url>
        </dependency>

        <dependency>
            <groupId>geronimo-spec</groupId>
            <artifactId>geronimo-spec-saaj</artifactId>
            <version>${geronimo_spec_saaj_version}</version>
            <url>http://java.sun.com/products/saaj/</url>
        </dependency>
   </dependencies>
</project>
