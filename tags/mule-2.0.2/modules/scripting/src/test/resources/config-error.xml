<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:script="http://www.mulesource.org/schema/mule/scripting/2.0"
    xsi:schemaLocation="
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
       http://www.mulesource.org/schema/mule/scripting/2.0 http://www.mulesource.org/schema/mule/scripting/2.0/mule-scripting.xsd">

	<!-- This should produce an error, the "engine" attribute is required unless 
	"file" is specified, in which case the file extension will be used to determine 
	the appropriate script engine. -->
    <script:script name="configError">
        return "$message Received"
    </script:script>
</mule>
