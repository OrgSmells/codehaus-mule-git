<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core/${MuleMajorMinorVersion}"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:spring="http://www.springframework.org/schema/beans"
#foreach($item in $transportArray)
    xmlns:${item}="http://www.mulesoft.org/schema/mule/${item}/${MuleMajorMinorVersion}"
#end
#foreach($item in $moduleArray)
    #if($item == "xml")
    xmlns:mule-xml="http://www.mulesoft.org/schema/mule/xml/${MuleMajorMinorVersion}"
    #else
    xmlns:${item}="http://www.mulesoft.org/schema/mule/${item}/${MuleMajorMinorVersion}"
    #end
#end
    xsi:schemaLocation="
       http://www.mulesoft.org/schema/mule/core/${MuleMajorMinorVersion} http://www.mulesoft.org/schema/mule/core/${MuleMajorMinorVersion}/mule.xsd
#foreach($item in $transportArray)
       http://www.mulesoft.org/schema/mule/${item}/${MuleMajorMinorVersion} http://www.mulesoft.org/schema/mule/${item}/${MuleMajorMinorVersion}/mule-${item}.xsd
#end
#foreach($item in $moduleArray)
       http://www.mulesoft.org/schema/mule/${item}/${MuleMajorMinorVersion} http://www.mulesoft.org/schema/mule/${item}/${MuleMajorMinorVersion}/mule-${item}.xsd
#end
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <description>
    ${description}
    </description>

    <model name="main">

        <service name="MYService">
            <inbound>
                <vm:inbound-endpoint path="in" synchronous="true"/>
            </inbound>

            <!-- TODO add your service component here. This can also be a Spring bean using <spring-object bean="name"/> -->
            <echo-component/>

            <outbound>
                <pass-through-router>
                    <stdio:outbound-endpoint system="OUT"/>
                </pass-through-router>
            </outbound>
        </service>

    </model>

</mule>
