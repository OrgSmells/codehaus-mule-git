<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:spring="http://www.springframework.org/schema/beans" 
    xmlns:jdbc="http://www.mulesource.org/schema/mule/jdbc" 
    xmlns:vm="http://www.mulesource.org/schema/mule/vm"
    xsi:schemaLocation="
       http://www.mulesource.org/schema/mule/core http://www.mulesource.org/schema/mule/core/3.0/mule.xsd
       http://www.mulesource.org/schema/mule/jdbc http://www.mulesource.org/schema/mule/jdbc/3.0/mule-jdbc.xsd
       http://www.mulesource.org/schema/mule/vm http://www.mulesource.org/schema/mule/vm/3.0/mule-vm.xsd
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <model>
        <service name="testService">
            <inbound>
                <vm:inbound-endpoint path="in" synchronous="true" />
            </inbound>
            <outbound>
                <chaining-router>
                    <jdbc:outbound-endpoint queryKey="getTest" synchronous="true" />
                    <vm:outbound-endpoint path="resultInterpreter" synchronous="true" />
                </chaining-router>
            </outbound>
        </service>

        <service name="interpreter">
            <inbound>
                <vm:inbound-endpoint path="resultInterpreter" synchronous="true" />
            </inbound>
        </service>
    </model>
</mule>
