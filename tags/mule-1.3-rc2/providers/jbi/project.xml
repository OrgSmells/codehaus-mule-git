<?xml version="1.0"?>

<project>
    <extend>${basedir}/../project.xml</extend>
    <pomVersion>3</pomVersion>
    <artifactId>mule-jbi-provider</artifactId>
    <name>Mule Jbi Provider</name>
    <description>Allows JBI components to interact with Mule.  Note this is different to Mule-JBI, which is a JBI (JSR-208) implementation that is  based on the Mule framework.
        for more information see the
        <a href="http://wiki.muleumo.org/display/MULE/Providers+Guide">Providers Guide</a>.
    </description>
    <shortDescription>jbi</shortDescription>
    <url>http://www.muleumo.org/docs/providers/jbi/</url>
    <dependencies>
         <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-jbi-spec</artifactId>
            <version>${mule_jbi_spec_version}</version>
            <url>http://www.mulejbi.org</url>
        </dependency>
    </dependencies>
</project>
