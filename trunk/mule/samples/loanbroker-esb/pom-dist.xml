<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    
  <modelVersion>4.0.0</modelVersion>
  <parent>
        <groupId>org.mule</groupId>
        <artifactId>mule-samples</artifactId>
        <version>${version}</version>
  </parent>
  <groupId>org.mule.samples</groupId>
  <artifactId>mule-samples-loanbroker-esb-dist</artifactId>
  <name>Mule Loan Broker ESB Example Dist</name>
  <description>Grabs the dependencies used to execute the Loan Borker ESB example at runtime</description>
  <dependencies>
        
    <dependency>
            <groupId>openejb</groupId>
            <artifactId>openejb-core</artifactId>
            <version>1.0-beta1</version>
        </dependency>
        
        <dependency>
            <groupId>activemq</groupId>
            <artifactId>activemq</artifactId>
            <version>3.2.4</version>
            <!-- These transitive dependencies are not found on ibiblio and cause
                the build to fail. -->
            <exclusions>
                <exclusion>
                    <groupId>javacc</groupId>
                    <artifactId>javacc</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>activecluster</groupId>
                    <artifactId>activecluster</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>incubator-derby</groupId>
                    <artifactId>derby</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>incubator-derby</groupId>
                    <artifactId>derbynet</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>xmlbeans</groupId>
                    <artifactId>xmlbeans-jsr173-api</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>activemq</groupId>
                    <artifactId>activemq-core-test</artifactId>
                </exclusion>
            </exclusions>
        </dependency>

        
  </dependencies>
</project>