<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/3.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/3.0.0 http://maven.apache.org/maven-v3_0_0.xsd">
    <extend>${basedir}/../project.xml</extend>
    <id>mule-extras-acegi</id>
    <artifactId>mule-extras-acegi</artifactId>
    <name>Mule Acegi Security</name>
    <shortDescription>acegi</shortDescription>
    <description>Mule Acegi Support exposes Acegi Authentication providers and Method interceptors to authentication event traffic via endpoints and method level authorisation on components.
    </description>
	
    <dependencies>
        <!-- optional: used by the http remoting test case -->
        <dependency>
            <artifactId>mule-http-provider</artifactId>
            <groupId>mule</groupId>
            <version>${pom.currentVersion}</version>
            <url>http://muleumo.org/docs/modules/mule-http-provider/</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>
        <dependency>
            <artifactId>mule-tcp-provider</artifactId>
            <groupId>mule</groupId>
            <version>${pom.currentVersion}</version>
            <url>http://muleumo.org/docs/modules/mule-tcp-provider/</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>
        <dependency>
            <artifactId>mule-vm-provider</artifactId>
            <groupId>mule</groupId>
            <version>${pom.currentVersion}</version>
            <url>http://muleumo.org/docs/modules/mule-vm-provider/</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>
        <dependency>
            <artifactId>mule-extras-client</artifactId>
            <groupId>mule</groupId>
            <version>${pom.currentVersion}</version>
            <url>http://muleumo.org/docs/modules/mule-extras-client/</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>
        <dependency>
            <artifactId>commons-httpclient</artifactId>
            <groupId>commons-httpclient</groupId>
            <version>${commons_httpclient_version}</version>
            <url>http://jakarta.apache.org/commons/httpclient/index.html</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
            </properties>
        </dependency>
        <dependency>
            <artifactId>commons-io</artifactId>
            <groupId>commons-io</groupId>
            <version>${commons_io_version}</version>
        </dependency>
       <dependency>
            <artifactId>commons-logging</artifactId>
            <groupId>commons-logging</groupId>
            <version>${commons_logging_version}</version>
        </dependency>
    </dependencies>
    <build>
        <unitTestSourceDirectory>${basedir}/src/test/java</unitTestSourceDirectory>
    </build>
</project>
