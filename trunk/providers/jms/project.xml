<?xml version="1.0"?>
<project>
    <extend>${basedir}/../project.xml</extend>
    <pomVersion>3</pomVersion>
    <artifactId>mule-jms-provider</artifactId>
    <name>Mule Jms Provider</name>
    <description>A Mule provider for Jms Connectivity. for more information see the
        <a href="http://wiki.muleumo.org/display/MULE/Providers+Guide">Providers Guide</a>.
    </description>
    <shortDescription>jms</shortDescription>
    <url>http://www.muleumo.org/doc/providers/jms/</url>
    <dependencies>
        <dependency>
            <groupId>fscontext</groupId>
            <artifactId>fscontext</artifactId>
            <jar>fscontext.jar</jar>
            <url></url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>providerutil</groupId>
            <artifactId>providerutil</artifactId>
            <version></version>
            <jar>providerutil.jar</jar>
            <url></url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <artifactId>activemq</artifactId>
            <groupId>activemq</groupId>
            <version>${activemq_version}</version>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
        	<groupId>openjms</groupId>
        	<artifactId>openjms-client</artifactId>
        	<version>${openjms_client_version}</version>
            <type>jar</type>
            <url>http://openjms.sourceforge.net/</url>
            <properties>
            	<ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
        	<groupId>exolabcore</groupId>
        	<artifactId>exolabcore</artifactId>
        	<version>${exolabcore_version}</version>
            <type>jar</type>
            <url>http://openjms.sourceforge.net/</url>
            <properties>
            	<ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>jotm</groupId>
            <artifactId>jotm</artifactId>
            <version>${jotm_version}</version>
            <type>jar</type>
            <url>http://jotm.objectweb.org</url>
            <properties>
            	<ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>jotm</groupId>
            <artifactId>jotm-carol</artifactId>
            <version>${jotm_carol_version}</version>
            <type>jar</type>
            <url>http://jotm.objectweb.org</url>
            <properties>
            	<ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>jotm</groupId>
            <artifactId>jotm-jrmp-stubs</artifactId>
            <version>${jotm_jrmp_stubs_version}</version>
            <type>jar</type>
            <url>http://jotm.objectweb.org</url>
            <properties>
            	<ignore>true</ignore>
            </properties>
        </dependency>
    </dependencies>
</project>
