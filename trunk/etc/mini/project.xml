<?xml version="1.0" encoding="UTF-8"?>
<project>
    <extend>${basedir}/../../mule/project.xml</extend>
    <pomVersion>3</pomVersion>
    <id>mini-mule</id>
    <artifactId>mini-mule</artifactId>
    <name>Mini Mule</name>
    <shortDescription>mini-mule</shortDescription>
    <description>Is a light-weight mini distribution of Mule that includes the core plus any providers that do not have other dependencies, such as vm, tcp, udp, multicst, file, jms, stream, servlet, ssl and the mule client</description>
    <dependencies>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-core</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-vm-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-file-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-tcp-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-ssl-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-jms-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-udp-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-multicast-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-stream-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-servlet-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
